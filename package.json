{
  "name": "equity_flow",
  "private": true,
  "scripts": {
    "coverage": "yarn test --coverage",
    "eslint": "./node_modules/.bin/eslint",
    "eslint:all": "./node_modules/.bin/eslint app/**/*.jsx spec/**/*.js",
    "test": "./node_modules/.bin/jest"
  },
  "dependencies": {
    "@rails/webpacker": "^3.2.0",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap-sass": "^3.3.7",
    "immutable": "^3.8.2",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.13.1",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.0.4",
    "moxios": "^0.4.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.3",
    "sinon": "^4.2.2",
    "webpack-dev-server": "^2.9.7"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "app/javascript/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "coverageDirectory": ".coverage/js",
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|css)$": "<rootDir>/spec/javascript/__mocks__/styles.js"
    },
    "roots": [
      "spec/javascript"
    ],
    "setupFiles": [
      "<rootDir>/spec/javascript/setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/spec/javascript/jest.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
